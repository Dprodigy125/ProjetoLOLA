<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="EditText">
        <item name="android:layout_width">330dp</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:background">@drawable/edit_text</item>
        <item name="android:padding">12dp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>

    </style>

    <style name="Text_Dados">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/dados_text</item>
        <item name="android:padding">12pt</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
    </style>

    <style name="EditTextI">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/edit_text</item>
        <item name="android:padding">20pt</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>

    </style>

    <style name="Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">40px</item>
        <item name="android:layout_marginRight">40px</item>
        <item name="android:layout_marginTop">15px</item>
        <item name="android:padding">20dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
        <item name="android:background">@drawable/button</item>
    </style>

    <style name="esqueceusenha">
        <item name="android:padding">10dp</item>
        <item name="android:textSize">11dp</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>

    </style>

    <style name="outraentrada">
        <item name="android:padding">10dp</item>
        <item name="android:textSize">12dp</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
    </style>
    <style name="cadastrar">
        <item name="android:padding">10dp</item>
        <item name="android:textSize">12dp</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="textoI">
        <item name="android:padding">10dp</item>
        <item name="android:textSize">40px</item>
        <item name="android:textStyle">normal</item>
        <item name="textAllCaps">false</item>

    </style>

    <style name="textoII">
        <item name="android:padding">10dp</item>
        <item name="android:textSize">64px</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="BackgroundImageStyle">
        <item name="android:windowBackground">@drawable/background</item>
    </style>



</resources>